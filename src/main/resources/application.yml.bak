spring:
  profiles:
    active: mock
  graphql:
    websocket:
      path: /graphql
    schema:
      printer:
        enabled: true

management:
  endpoints:
    web:
      exposure:
        include: health,metrics,info

logging:
  level:
    org.springframework.web: debug
    org.springframework.http: debug
    org.springframework.graphql: debug
    reactor.netty: debug
